freetime登録id：<%= @freetime.id %></br>
登録user名：<%= @freetime.user.name %></br>

<%= form_for @freetime, :url => {:action => 'update'} do |f| %>
  <label class="control-label" >集合場所</label>
  <%= f.text_field :place, value: @freetime.place, placeholder: "場所名を入力してね", class: 'form-control' %></br>

  <label class="control-label" for="">日時</label>
  <%= f.datetime_select :time, :default => @freetime.time, class: 'form-control' %></br>

  <%= f.submit '内容を更新', class: 'btn btn-primary' %>
  <% end %>


</div>